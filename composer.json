{
    "name": "mradhi/ibanfirst-sdk",
    "description": "iBanFirst PHP Client Library",
    "keywords": [
        "ibanfirst",
        "finance",
        "api"
    ],
    "homepage": "https://ibanfirst.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Radhi Guennichi",
            "email": "guennichiradhi@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "symfony/http-client": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "IBanFirst\\": "lib/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload-dev": {
        "psr-4": {
            "IBanFirst\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
